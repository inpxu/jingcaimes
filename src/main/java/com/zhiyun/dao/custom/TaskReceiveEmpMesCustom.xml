<?xml version="1.0" encoding="UTF-8" ?>   
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhiyun.dao.impl.TaskReceiveEmpMesDaoImpl">

    <update id="updatePriceByInsiderOrders" parameterType="map">
        UPDATE TASK_RECEIVE_EMP_MES
        SET price = #{price,jdbcType=DECIMAL}
        WHERE inside_order in
        <foreach collection="insideOrders" index="index" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </update>

</mapper>   
